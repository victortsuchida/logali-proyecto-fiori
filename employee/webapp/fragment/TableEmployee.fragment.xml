<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
    <Table id="_IDGenTable1" inset="false" items="{odataNorthwind>/Employees}">
        <columns>
            <Column id="_IDGenColumn1">
                <Text id="_IDGenText1" text="{i18n>employeeId}" />
            </Column>
            <Column id="_IDGenColumn2">
                <Text id="_IDGenText2" text="{i18n>employeeName}" />
            </Column>
            <Column id="_IDGenColumn3">
                <Text id="_IDGenText3" text="{i18n>employeeCountry}" />
            </Column>
            <Column id="_IDGenColumn4" visible="{selectVisibility>/showCityColumn}">
                <Text id="_IDGenText4" text="{i18n>employeeCity}" />
            </Column>
            <Column id="_IDGenColumn5">
                <Text id="_IDGenText5" text="{i18n>employeeShowDetails}" />
            </Column>
        </columns>

        <items>
            <ColumnListItem id="_IDGenColumnListItem1" type="Navigation" press=".onShowEmployee">
                <cells>
                    <Label id="_IDGenLabel3" text="{odataNorthwind>EmployeeID}"/>
                    <Label id="_IDGenLabel4" text="{odataNorthwind>LastName}, {odataNorthwind>FirstName}"/>
                    <Label id="_IDGenLabel5" text="{odataNorthwind>Country}"/>
                    <Label id="_IDGenLabel6" text="{odataNorthwind>City}"/>
                    <core:Icon id="_IDGenIcon1" src="sap-icon://show" press=".onDialogOpen"/>
                </cells>
            </ColumnListItem>
        </items>
    </Table>        
    <HBox id="_IDGenHBox1" >
    </HBox>
</core:FragmentDefinition>